<view class="tab">
    <view class="tab_title">
        <view class="title-name {{item.isActive ? 'active' : ''}}"
        wx:for="{{tabs}}"
        wx:key="id"
        bindtap="handleItemTap"
        data-index="{{index}}"
        >{{item.value}}</view>
    </view>
    <view class="tab_content">
    <slot></slot>
    </view>
</view>
  
