.order_main {
  color: #666;
  padding-top: 10rpx;
  border-bottom: 1px solid #ccc;
}
.order_main .order_number {
  padding: 10rpx 20rpx;
  display: flex;
  justify-content: space-between;
}
.order_main .order_price {
  padding: 10rpx 20rpx;
  display: flex;
  justify-content: space-between;
}
.order_main .order_price .text {
  color: var(--themeColor);
  font-size: 32rpx;
}
.order_main .order_date {
  padding: 10rpx 20rpx;
  display: flex;
  justify-content: space-between;
}
